
<%= simple_form_for [:admin, @product], html: {multipart: true} do |f| %>
 
  <div class="group">
    
    <%= f.input :title %>
  </div>
  
  <div class="group">

    <%= f.input :description %>
  </div>
  
  <div class="group">
    
    <%= f.input :quantity %>
  </div>
    <div class="group">
    <%= f.fields_for :photos do |photo| %>

    <%= photo.file_field :image %>
  <% end %>
</div>
  <%= f.submit "Submit", :disable_with => 'Submiting...' %>
 
 
<% end %>